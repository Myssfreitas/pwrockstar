<script>
$(document).ready(function(){
    
    //click to show/hide tags
    $('.post_tags').hide();
    $('.tag-button').click(function() {
        $(this).closest('.posts').find('.post_tags').slideToggle(600);
        return false;
    });
    
    //search suggestions script     
    $('.search').submit(function(event){
       var value = $('input:first').val();
       location.replace('https://{Name}.tumblr.com/tagged/' + value);
   });
   
   //click to show/hide tumblr controls
    $('.controls-button').click(function(){
       $('body').toggleClass('controls-click');
   });
   
   //side panel for responsive design
    $('.slider').click(function(){
        $('#responsive-background').toggleClass('active');
    });
    
});
</script>

